{
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md",
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "index.md"
        ],
        "exclude": [
          "api/index.yml"
        ],
        "src": "altv-types/docs",
        "dest": "js/"
      },
      {
        "files": [
          "api/**.yml",
          "api/index.md",
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "src": "coreclr-module/docs",
        "dest": "cs/"
      },
      {
        "files": [
          "**.md",
          "**/toc.yml",
        ],
        "src": "cpp-sdk/articles/",
        "dest": "sdk/"
      },
      {
        "files": [
          "api/**.yml",
          "api/index.md",
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "src": "altv-docs-gta",
        "dest": "gta/"
      },
      {
        "files": [
          "toc.yml",
          "index.md",
          "articles/**.md",
          "articles/**/toc.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "src": "altv-types/docs",
        "dest": "js/"
      },
      {
        "files": [
          "images/**"
        ],
        "src": "coreclr-module/docs",
        "dest": "cs/"
      },
      {
        "files": [
          "images/**"
        ],
        "src": "altv-docs-gta",
        "dest": "gta/"
      },
      {
        "files": [
          "images/**"
        ],
        "src": "altv-docs-gta-assets",
        "dest": "gta/"
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "src": "altv-types/docs",
        "dest": "js/"
      },
      {
        "files": [
          "apidoc/**.md"
        ],
        "src": "coreclr-module/docs",
        "dest": "cs/"
      },
      {
        "files": [
          "apidoc/**.md"
        ],
        "src": "altv-docs-gta",
        "dest": "gta/"
      }
    ],
    "globalMetadata": {
      "_appName": "alt:V",
      "_appTitle": "",
      "_appFooter": "altMP Team<br/>Generated by <b>DocFX</b>",
      "_disableSideFilter": true,
      "_disableFooter": true,
      "_gitContribute": {
        "branch": "master",
        "apiSpecFolder": "docs/apidoc"
      },
      "_gitUrlPattern": "github"
    },
    "template": [
      "default",
      "templates/docfx-plugins-typescriptreference",
      "templates/docfx-plugins-extractsearchindex",
      "templates/discordfx",
      "templates/altmp"
    ],
    "postProcessors": [ "ExtractSearchIndexAlt" ],
    "markdownEngineName": "markdig",
    "dest": "_site"
  }
}
